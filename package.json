{
  "name": "css-tractor",
  "displayName": "CSS tractor",
  "description": "Extracting class names from HTML and generate CSS stylesheet",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "kubosho",
  "engines": {
    "vscode": "^1.12.0",
    "node": ">=8.0.0"
  },
  "categories": ["Other"],
  "activationEvents": ["*"],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.run",
        "title": "Run: CSS tractor"
      }
    ]
  },
  "scripts": {
    "format": "prettier --write \"{.,src/**}/*.js\"",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "ava"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "eslint": "^3.6.0",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "htmlparser2": "^3.9.2",
    "prettier": "^1.7.0"
  },
  "ava": {
    "files": ["**/*.test.js"],
    "source": ["**/*.{js}"],
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": true
  }
}
